generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model alergenos {
  id     Int    @id @default(autoincrement())
  nombre String @db.VarChar(50)
  icono  String @db.VarChar(50)
  orden  Int?
}

model alergenos_producto {
  id_alergeno Int
  id_producto Int

  @@id([id_alergeno, id_producto])
}

model aliados {
  id                         Int      @id @default(autoincrement())
  cliente                    Int
  nombre                     String   @db.VarChar(50)
  tipo                       String?  @db.VarChar(3)
