generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Usuarios {
  us_cd_usuario    String  @id @db.VarChar(30)
  us_contrasena    String  @db.VarChar(30)
  us_nombreusuario String  @db.VarChar(200)
  us_perfil        String  @db.VarChar(2)
  us_email         String? @db.VarChar(200)
  us_estatus       String  @default("A") @db.VarChar(1)

  @@map("usuarios")
}
